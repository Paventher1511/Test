{
  "name": "comprehensive-api-library",
  "version": "1.0.0",
  "description": "A comprehensive library with APIs, functions, and components for modern web development",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "docs",
    "examples"
  ],
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.{js,ts,tsx}",
    "lint:fix": "eslint src/**/*.{js,ts,tsx} --fix",
    "format": "prettier --write src/**/*.{js,ts,tsx,json,md}",
    "type-check": "tsc --noEmit",
    "docs:build": "typedoc src/index.ts",
    "docs:serve": "npx http-server docs -p 8080",
    "example:basic": "node examples/basic-usage.js",
    "example:react": "cd examples/react-app && npm start",
    "prepublishOnly": "npm run build && npm run test",
    "release": "standard-version",
    "clean": "rimraf dist"
  },
  "keywords": [
    "api",
    "client",
    "functions",
    "utilities",
    "components",
    "react",
    "typescript",
    "validation",
    "data-processing",
    "ui-components"
  ],
  "author": "Your Name <your.email@example.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/comprehensive-api-library.git"
  },
  "bugs": {
    "url": "https://github.com/yourusername/comprehensive-api-library/issues"
  },
  "homepage": "https://github.com/yourusername/comprehensive-api-library#readme",
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "dependencies": {
    "axios": "^1.6.0",
    "date-fns": "^2.30.0",
    "lodash": "^4.17.21",
    "validator": "^13.11.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@types/lodash": "^4.14.200",
    "@types/node": "^20.8.0",
    "@types/react": "^18.2.0",
    "@types/validator": "^13.11.0",
    "@typescript-eslint/eslint-plugin": "^6.7.0",
    "@typescript-eslint/parser": "^6.7.0",
    "eslint": "^8.50.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-react": "^7.33.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jest": "^29.7.0",
    "prettier": "^3.0.0",
    "rimraf": "^5.0.0",
    "rollup": "^3.29.0",
    "rollup-plugin-typescript2": "^0.36.0",
    "standard-version": "^9.5.0",
    "ts-jest": "^29.1.0",
    "typedoc": "^0.25.0",
    "typescript": "^5.2.0"
  },
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    },
    "react-dom": {
      "optional": true
    }
  },
  "publishConfig": {
    "access": "public"
  }
}